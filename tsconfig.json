{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        // "lib": ["dom","es2017"],
        "moduleResolution": "node",
        "allowJs": true,
        "checkJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["./**/*"],
    "exclude": ["./**/*.js"]
}