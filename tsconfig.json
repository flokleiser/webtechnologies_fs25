{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        // "outDir": "./",
        // "rootDir": "./pages",
        "allowJs": true,
        "checkJs": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["./**/*"],
    "exclude": ["./**/*.js"]
}